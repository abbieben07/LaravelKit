{
    "name": "abbieben/laravel-kit",
    "description": "This package sets up my common code",
    "keywords": [
        "abbieben07",
        "laravel",
        "core"
    ],
    "homepage": "https://github.com/abbieben07/LaravelKit",
    "license": "MIT",
    "authors": [
        {
            "name": "Abraham Achile Ben-Obaje",
            "email": "abbieben07@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "cknow/laravel-money": "^7.0",
        "illuminate/contracts": "^9.0",
        "inertiajs/inertia-laravel": "^0.6.3",
        "spatie/laravel-package-tools": "^1.13.0",
        "spatie/laravel-sluggable": "^3.4"
    },
    "require-dev": {
        "laravel/dusk": "^7.1",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "2.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Novacio\\Core\\": "src",
            "Novacio\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Novacio\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Novacio\\CoreServiceProvider"
            ],
            "aliases": {
                "Core": "Novacio\\Facades\\Core"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}